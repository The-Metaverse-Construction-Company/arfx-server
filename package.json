{
  "name": "@arwall-arfx/server",
  "version": "0.0.1",
  "main": "src/index.js",
  "private": false,
  "engines": {
    "node": ">=12",
    "yarn": "*"
  },
  "scripts": {
    "prestart": "yarn docs",
    "start": "cross-env NODE_ENV=production pm2 start ./src/index.js",
    "dev": "nodemon -L ./dist/index.js",
    "dev:watch": "tsc -w",
    "test": "cross-env NODE_ENV=test nyc --reporter=html --reporter=text mocha --timeout 20000 --exit --recursive src/api/tests",
    "test:unit": "cross-env NODE_ENV=test mocha src/api/tests/unit",
    "test:integration": "cross-env NODE_ENV=test mocha --timeout 20000 --exit src/api/tests/integration",
    "test:watch": "cross-env NODE_ENV=test mocha --watch src/api/tests/unit",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "postcoverage": "open-cli coverage/lcov-report/index.html",
    "postpublish": "git push --tags",
    "deploy": "sh ./deploy.sh",
    "docs": "apidoc -i src -o docs",
    "postdocs": "open-cli docs/index.html",
    "docker:start": "cross-env NODE_ENV=production pm2-docker start ./src/index.js",
    "docker:prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up",
    "docker:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up",
    "docker:test": "docker-compose -f docker-compose.yml -f docker-compose.test.yml up --abort-on-container-exit"
  },
  "dependencies": {
    "@azure/storage-blob": "^12.4.0",
    "@sendgrid/mail": "^7.4.2",
    "@types/mocha": "^8.2.0",
    "axios": "^0.19.0",
    "bcrypt": "^5.0.0",
    "bcryptjs": "2.4.3",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.0",
    "busboy": "^0.3.1",
    "compression": "^1.6.2",
    "connect-busboy": "0.0.2",
    "connect-multiparty": "^2.2.0",
    "cors": "^2.8.3",
    "cross-env": "^6.0.3",
    "dotenv-safe": "^6.0.0",
    "email-templates": "^6.0.3",
    "express": "^4.15.2",
    "express-fingerprint": "^1.2.2",
    "express-validation": "^1.0.2",
    "hbs": "^4.1.1",
    "helmet": "^3.5.0",
    "http-status": "^1.0.1",
    "into-stream": "^6.0.0",
    "joi": "^10.4.1",
    "jsonwebtoken": "^8.5.1",
    "jwt-simple": "^0.5.6",
    "lodash": "^4.17.4",
    "method-override": "^3.0.0",
    "moment-timezone": "^0.5.13",
    "mongoose": "^5.2.17",
    "morgan": "^1.8.1",
    "multer": "^1.4.2",
    "nodemailer": "^6.3.1",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "4.0.0",
    "pm2": "^4.2.0",
    "pug": "^2.0.4",
    "random-number": "0.0.9",
    "redis": "^3.0.2",
    "stripe": "^8.131.1",
    "swagger-jsdoc": "^6.0.2",
    "swagger-ui-express": "^4.1.6",
    "uuid": "^3.1.0",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/busboy": "^0.2.3",
    "@types/connect-busboy": "0.0.2",
    "@types/cors": "^2.8.9",
    "@types/helmet": "^4.0.0",
    "@types/into-stream": "^3.1.1",
    "@types/jwt-simple": "^0.5.33",
    "@types/method-override": "0.0.31",
    "@types/multer": "^1.4.5",
    "@types/passport-http-bearer": "^1.0.36",
    "@types/passport-jwt": "^3.0.3",
    "@types/random-number": "0.0.0",
    "@types/redis": "^2.8.28",
    "@types/swagger-jsdoc": "^6.0.0",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/uuid": "^8.3.0",
    "apidoc": "^0.17.5",
    "chai": "^4.1.0",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.0.0",
    "eslint": "^6.4.0",
    "eslint-config-airbnb-base": "^12.0.1",
    "eslint-plugin-import": "^2.2.0",
    "husky": "^3.0.7",
    "mocha": "^6.2.2",
    "nodemon": "^2.0.1",
    "nyc": "^14.1.1",
    "opn-cli": "^5.0.0",
    "sinon": "^7.5.0",
    "sinon-chai": "^3.0.0",
    "supertest": "^4.0.2",
    "typescript": "^3.9.6"
  }
}
